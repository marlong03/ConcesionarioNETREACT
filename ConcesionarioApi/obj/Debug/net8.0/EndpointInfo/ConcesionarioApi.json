  "components": {
    "schemas": {
      "ClienteCreateDTO": {
        "required": [
          "documento",
          "nombre"
        ],
        "type": "object",
        "properties": {
          "nombre": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "documento": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "InformeGeneralDTO": {
        "type": "object",
        "properties": {
          "ventasPorTipo": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          },
          "ventasPorModelo": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Prueba": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "nombre": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "PublicacionCreateDTO": {
        "required": [
          "color",
          "esNuevo",
          "estadoDisponibilidad",
          "kilometraje",
          "precio",
          "vehiculoId"
        ],
        "type": "object",
        "properties": {
          "precio": {
            "maximum": 250000000,
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "color": {
            "minLength": 1,
            "type": "string"
          },
          "kilometraje": {
            "type": "integer",
            "format": "int32"
          },
          "esNuevo": {
            "type": "boolean"
          },
          "estadoDisponibilidad": {
            "minLength": 1,
            "type": "string"
          },
          "vehiculoId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ReglaPrecioCreateDTO": {
        "required": [
          "precio",
          "vehiculoId"
        ],
        "type": "object",
        "properties": {
          "precio": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "vehiculoId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "VehiculoCreateDTO": {
        "required": [
          "modelo",
          "tipo"
        ],
        "type": "object",
        "properties": {
          "modelo": {
            "minLength": 1,
            "type": "string"
          },
          "imagenUrl": {
            "type": "string",
            "nullable": true
          },
          "tipo": {
            "minLength": 1,
            "type": "string"
          },
          "puertas": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "cilindraje": {
            "maximum": 400,
            "minimum": 1,
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "velocidades": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "VentaCreateDTO": {
        "required": [
          "clienteId",
          "publicacionId"
        ],
        "type": "object",
        "properties": {
          "clienteId": {
            "type": "integer",
            "format": "int32"
          },
          "publicacionId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
